public class Triangle
{
    private static int height;
    private static int width;
    
    public Triangle(int triWidth,int triHeight){
        height = triHeight;
        width = triWidth;
    }
    
    public int getHeight(){
        return height;
    }
    public int getWidth(){
        return width;
    }
    public double getArea(){
        return .5 * width * height;
    }
    
}
