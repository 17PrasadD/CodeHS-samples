public class Fraction
{
       private static String stringedFrac;
       
       public Fraction(int theNum, int theDenom){
            stringedFrac = Integer.toString(theNum) + "/" + Integer.toString(theDenom);   
       }
       
       public String toString(){
           return stringedFrac;
       }
}
